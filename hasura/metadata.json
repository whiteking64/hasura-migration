{
  "resource_version": 3,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "sample-user",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "name": "User",
              "schema": "public"
            }
          }
        ],
        "configuration": {
          "connection_info": {
            "database_url": {
              "from_env": "HASURA_GRAPHQL_METADATA_DATABASE_URL"
            },
            "isolation_level": "read-committed",
            "use_prepared_statements": false
          }
        }
      }
    ]
  }
}